{
    "name": "morningtrain/wp-economic",
    "description": "PHP SDK for e-conomic with WordPress support",
    "keywords": [
        "morningtrain",
        "e-conomic",
        "wordpress"
    ],
    "homepage": "https://github.com/Morning-Train/wp-economic",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schadegg Br√∏nniche",
            "email": "ms@morningtrain.dk",
            "role": "Developer"
        },
        {
            "name": "Lars Rasmussen",
            "email": "laras@morningtrain.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "morningtrain/economic": "^1.2.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.15",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Morningtrain\\WpEconomic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morningtrain\\WpEconomic\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
